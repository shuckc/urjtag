# bits 31-28 of the Device Identification Register
# see D55. in [1]
0000	pxa250		A0
0001	pxa250		A1
0010	pxa250		B0
0011	pxa250		B1
0100	pxa250		B2
0101	pxa250c0	C0
0110	pxa250c0	PXA255A0
